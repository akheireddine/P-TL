#_____________________________PARETO

set xlabel "x_1"

set ylabel "x_2"

set style line 1 lt 1 lw 3 lc rgb '#2989C9'

set style arrow 1 head filled size screen 0.025,30,45 ls 1 lw 4



set style fill pattern 2
plot "Instances_Knapsack/Type_A/100_items/2KP100-TA-3.eff" title "Pareto front" w filledcurves above y1=29999 lc rgb "#FFCCCC"

replot "Instances_Knapsack/Type_A/100_items/2KP100-TA-3.eff" title "Pareto front" lt rgb "#FF6666"

replot 'file.dat' using 1:2:3:4 with vectors arrowstyle 1

set style line 5 lc rgb '#009900' pt 7   
replot "<echo '32549 41519'"   with points ls 5

unset key




